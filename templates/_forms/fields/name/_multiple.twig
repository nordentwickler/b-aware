{% fieldtag 'subFieldRows' %}
    {% for row in field.getFrontEndSubfields(_context) %}
        {% fieldtag 'subFieldRow' %}
            {% for subField in row %}
                {{ craft.formie.renderField(form, subField, {
                    element: submission ?? element ?? null,
                    value: value[subField.handle] ?? null,
                }) }}
            {% endfor %}
        {% endfieldtag %}
    {% endfor %}
{% endfieldtag %}
